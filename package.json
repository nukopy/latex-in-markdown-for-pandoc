{
  "name": "latex-highlight-in-pandoc-markdown",
  "displayName": "latex-highlight-in-pandoc-markdown",
  "description": "LaTeX highlighting for Markdown front matter, inline math, and block math in Pandoc-flavored documents.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "scripts": {
    "----- format, lint -----": "",
    "format": "biome format --write .",
    "lint": "biome lint --write .",
    "check": "biome check --write",
    "ci": "biome ci",
    "----- test -----": "",
    "test": "echo \"No tests yet\""
  },
  "contributes": {
    "languages": [
      {
        "id": "pandoc-markdown-latex",
        "aliases": [
          "Pandoc Markdown (LaTeX)",
          "pandoc-markdown-latex"
        ],
        "extensions": [
          ".md"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "pandoc-markdown-latex",
        "scopeName": "text.pandoc.markdown",
        "path": "./syntaxes/pandoc-markdown-latex.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.5",
    "@types/node": "^24.6.2",
    "tsx": "^4.20.6"
  }
}
