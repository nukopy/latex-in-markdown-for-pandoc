{
  "name": "latex-highlight-in-pandoc-markdown",
  "displayName": "latex-highlight-in-pandoc-markdown",
  "description": "LaTeX highlighting for Markdown front matter, inline math, and block math in Pandoc-flavored documents.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "scripts": {
    "----- format, lint -----": "",
    "format": "biome format --write .",
    "lint": "biome lint --write .",
    "check": "biome check --write",
    "ci": "biome ci",
    "----- test -----": "",
    "test": "echo \"No tests yet\""
  },
  "contributes": {
    "grammars": [
      {
        "scopeName": "markdown.latex.injection",
        "path": "./syntaxes/markdown-latex-injection.tmLanguage.json",
        "injectTo": [
          "text.html.markdown"
        ]
      },
      {
        "scopeName": "markdown.frontmatter.latex.injection",
        "path": "./syntaxes/frontmatter-latex-injection.tmLanguage.json",
        "injectTo": [
          "text.html.markdown"
        ]
      }
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.5",
    "@types/node": "^24.6.2",
    "tsx": "^4.20.6"
  }
}
