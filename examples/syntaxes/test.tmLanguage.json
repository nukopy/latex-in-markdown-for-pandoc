{
    "scopeName": "injection.test",
    "injectionSelector": "L:meta.embedded.block.frontmatter",
    "patterns": [
        {
            "include": "#latex-embedded-range"
        },
        {
            "include": "#keyword-test"
        }
    ],
    "repository": {
        "latex-embedded-range": {
            "name": "meta.embedded.block.frontmatter.pandoc-injection",
            "begin": "(# ----- pandoc-latex-begin -----)",
            "beginCaptures": {
                "1": {
                    "name": "punctuation.definition.comment.begin.pandoc-injection"
                }
            },
            "end": "(# ----- pandoc-latex-end -----)",
            "endCaptures": {
                "1": {
                    "name": "punctuation.definition.comment.end.pandoc-injection"
                }
            },
            "patterns": [
                {
                    "include": "text.tex.latex"
                },
                {
                    "include": "#latex-keyword"
                },
                {
                    "include": "#keyword-test"
                },
                {
                    "include": "source.yaml"
                }
            ]
        },
        "latex-keyword": {
            "patterns": [
                {
                    "include": "#header-include"
                }
            ]
        },
        "------- test -------": "",
        "keyword-test": {
            "patterns": [
                {
                    "include": "#nukopy"
                },
                {
                    "include": "#hello"
                },
                {
                    "include": "#TODO"
                }
            ]
        },
        "header-include": {
            "name": "keyword.nukopy.header-include",
            "match": "\\bheader-include\\b"
        },
        "nukopy": {
            "name": "keyword.nukopy.nukopy",
            "match": "\\bnukopy\\b"
        },
        "hello": {
            "name": "keyword.nukopy.hello",
            "match": "\\bhello\\b"
        },
        "TODO": {
            "name": "keyword.nukopy.TODO",
            "match": "\\bTODO\\b"
        }
    }
}
